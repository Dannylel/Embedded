#include<avr/io.h>
#undef F_CPU
#define F_CPU 8000000

#include<util/delay.h>


int main(void){
 DDRA|=(1<<0);
 DDRA|=(1<<1);
 DDRA|=(1<<2);
 DDRA|=(1<<3);
DDRC|=(1<<6);
while(1){

	PORTA|=(1<<0);
	PORTA=0b00000001;
	PORTA=0b00000001;

	_delay_ms(100);
	PORTA=0;
	PORTA|=(1<<1);
	PORTA=0b00000010;
	_delay_ms(100);
	PORTA=0;
	PORTA=0b00000100;
	PORTA=0b00000100;
	_delay_ms(100);


}
	return 0;}
