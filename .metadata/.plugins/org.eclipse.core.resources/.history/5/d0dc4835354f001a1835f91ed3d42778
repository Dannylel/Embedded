#include"../Lib/Std_types.h"
#include"../Lib/Bit_math.h"
#include"Dio_int.h"
#include"LED_int.h"
#include<avr/io.h>
#define F_CPU 8000000
#include<util/delay.h>

int main(void){
	SET_BIT(GICR,6);
	SET_BIT(SREG,7);
	Led_vidInit(Led_2);
	while(1){

		Led_u8TurnOn(Led_2);
		_delay_ms(400);
		Led_u8TurnOff(Led_2);
		_delay_ms(400);

	}
	return 0;
}
